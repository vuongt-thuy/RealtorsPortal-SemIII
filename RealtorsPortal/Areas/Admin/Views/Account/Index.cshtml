
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

@section title { Account Management}
@section section_title { Account Management }
@section breadcrumb { Account Management }

<div class="row">
    <div class="col-lg-4 col-6">
