            </div>
            <a href="@Url.Action("PrivateSellers", "Account")" class="small-box-footer">Get more <i class="fas fa-arrow-circle-right"></i></a>
        </div>
    </div>
    <!-- ./col -->
    <div class="col-lg-4 col-6">
        <!-- small box -->
        <div class="small-box bg-warning">
            <div class="inner">
                <h5>Agents</h5>
            </div>
            <a href="@Url.Action("Agents", "Account")" class="small-box-footer">Get more <i class="fas fa-arrow-circle-right"></i></a>
