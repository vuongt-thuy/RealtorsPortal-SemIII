
@{
    DataAccessLayer.Models.Entities.User user = (DataAccessLayer.Models.Entities.User)Session["user"];
}

<!DOCTYPE html>
<html lang="en">

<!-- Mirrored from themesbox.in/admin-templates/olian/html/light-vertical/index.html by HTTrack Website Copier/3.x [XR&CO'2014], Wed, 04 Aug 2021 12:04:45 GMT -->
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="Olian is a bootstrap minimal & clean admin template">
    <meta name="keywords" content="admin, admin panel, admin template, admin dashboard, admin theme, bootstrap 4, responsive, sass support, ui kits, crm, ecommerce">
    <meta name="author" content="Themesbox17">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <title>Realtors Portal</title>
    <!-- Fevicon -->

    <link rel="shortcut icon" href="~/Areas/UserBackend/Content/assets/images/logo.png">
    <!-- Start css -->
    <!-- Switchery css -->
    <link href="~/Areas/UserBackend/Content/assets/plugins/switchery/switchery.min.css" rel="stylesheet">
    <!-- Apex css -->
    <link href="~/Areas/UserBackend/Content/assets/plugins/apexcharts/apexcharts.css" rel="stylesheet">
    <!-- Slick css -->
    <link href="~/Areas/UserBackend/Content/assets/plugins/slick/slick.css" rel="stylesheet">
    <link href="~/Areas/UserBackend/Content/assets/plugins/slick/slick-theme.css" rel="stylesheet">

    <link href="~/Areas/UserBackend/Content/assets/css/bootstrap.min.css" rel="stylesheet" type="text/css">
    <link href="~/Areas/UserBackend/Content/assets/css/icons.css" rel="stylesheet" type="text/css">
    <link href="~/Areas/UserBackend/Content/assets/css/flag-icon.min.css" rel="stylesheet" type="text/css">
    <link href="~/Areas/UserBackend/Content/assets/css/style.css" rel="stylesheet" type="text/css">
    @RenderSection("css", required: false)

    <!-- End css -->
</head>
<body class="vertical-layout">
    <!-- End Infobar Setting Sidebar -->
    <!-- Start Containerbar -->
    <div id="containerbar">
        <!-- Start Leftbar -->
        <div class="leftbar">
            <!-- Start Sidebar -->
            <div class="sidebar">
                <!-- Start Logobar -->
                <div class="logobar">
                    <a href="/UserBackend" class="logo logo-large"><img src="~/Areas/UserBackend/Content/assets/images/logo.png"  class="img-fluid" alt="logo" style="height: 70px; width: 70px;"></a>
                    <a href="/UserBackend" class="logo logo-small"><img src="~/Areas/UserBackend/Content/assets/images/small_logo.png" class="img-fluid" alt="logo"></a>
                </div>
                <!-- End Logobar -->
                <!-- Start Navigationbar -->
                <div class="navigationbar">
                    <ul class="vertical-menu">
                        <li class="vertical-header">Main</li>
                        <li>
                            <a href="widgets.html">
                                <i class="ri-home-2-line"></i><span>Home</span>
                            </a>
                        </li>
                        <li class="vertical-header">Services Management</li>
                        <li>
                            <a href="javaScript:void();">
                                <i class="ri-newspaper-line"></i><span>Articles</span> <i class="ri-arrow-right-s-line"></i>
                            </a>
                            <ul class="vertical-submenu">
                                <li>
                                    <a href="@Url.Action("Create", "Article")">
                                        <span>Create Article</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="@Url.Action("All", "Article")">
                                        <span>All Articles</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="@Url.Action("PostedArticle", "Article")">
                                        <span>Posted Articles</span>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li class="vertical-header">Transactions Management</li>
                        <li>
                            <a href="widgets.html">
                                <i class="ri-booklet-line"></i><span>History</span>
                            </a>
                        </li>
                        <li class="vertical-header">Users Management</li>
                        <li>
                            <a href="/UserBackend/User/Information">
                                <i class="ri-profile-line"></i><span>Profile</span>
                            </a>
                        </li>
                        <li>
                            <a href="/UserBackend/User/Notification">
                                <i class="ri-notification-2-line"></i><span>Notifications</span>
                            </a>
                        </li>
                        <li>
                            <a href="/UserBackend/Contact">
                                <i class="ri-contacts-book-line"></i><span>Contact</span>
                            </a>
                        </li>
                        <li>
                            <a href="/Logout">
                                <i class="ri-logout-box-r-line"></i><span>Logout</span>
                            </a>
                        </li>
                    </ul>
                </div>
                <!-- End Navigationbar -->
            </div>
            <!-- End Sidebar -->
        </div>
        <!-- End Leftbar -->
        <!-- Start Rightbar -->
        <div class="rightbar">
            <!-- Start Topbar Mobile -->
            <div class="topbar-mobile">
                <div class="row align-items-center">
                    <div class="col-md-12">
                        <div class="mobile-logobar">
                            <a href="index.html" class="mobile-logo"><img src="~/Areas/UserBackend/Content/assets/images/logo.svg" class="img-fluid" alt="logo"></a>
                        </div>
                        <div class="mobile-togglebar">
                            <ul class="list-inline mb-0">
                                <li class="list-inline-item">
                                    <div class="topbar-toggle-icon">
                                        <a class="topbar-toggle-hamburger" href="javascript:void();">
                                            <span class="iconbar">
                                                <i class="ri-more-fill menu-hamburger-horizontal"></i>
                                                <i class="ri-more-2-fill menu-hamburger-vertical"></i>
                                            </span>
                                        </a>
                                    </div>
                                </li>
                                <li class="list-inline-item">
                                    <div class="menubar">
                                        <a class="menu-hamburger" href="javascript:void();">
                                            <span class="iconbar">
                                                <i class="ri-menu-2-line menu-hamburger-collapse"></i>
                                                <i class="ri-close-line menu-hamburger-close"></i>
                                            </span>
                                        </a>
                                    </div>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Start Topbar -->
            <div class="topbar">
                <!-- Start row -->
                <div class="row align-items-center">
                    <!-- Start col -->
                    <div class="col-md-12 align-self-center">
                        <div class="togglebar">
                            <ul class="list-inline mb-0">
                                <li class="list-inline-item">
                                    <div class="menubar">
                                        <a class="menu-hamburger" href="javascript:void();">
                                            <span class="iconbar">
                                                <i class="ri-menu-2-line menu-hamburger-collapse"></i><i class="ri-close-line menu-hamburger-close"></i>
                                            </span>
                                        </a>
                                    </div>
                                </li>
                            </ul>
                        </div>
                        <div class="infobar">
                            <ul class="list-inline mb-0">
                                <li class="list-inline-item">
                                    <div class="profilebar">
                                        <div class="dropdown">
                                            <a class="dropdown-toggle" href="#" role="button" id="profilelink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                <img src="@user.Avt" class="img-fluid" alt="profile">
                                                <span class="live-icon">
                                                    @user.Fullname
                                                </span>
                                            </a>
                                        </div>
                                    </div>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <!-- End col -->
                </div>
                <!-- End row -->
            </div>
            <!-- End Topbar -->
            <!-- Start Breadcrumbbar -->
            <div class="breadcrumbbar">
                <div class="row align-items-center">
                    <div class="col-md-8 col-lg-8">
                        <h4 class="page-title">@RenderSection("page_title", required: true)</h4>
                        <div class="breadcrumb-list">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a href="@Url.Action("Information", "User")">Home</a></li>
                                <li class="breadcrumb-item active" aria-current="page">@RenderSection("breadcrumb", required: true)</li>
                            </ol>
                        </div>
                    </div>
                    <div class="col-md-4 col-lg-4">
                        <div class="widgetbar">
                            <button class="btn btn-primary" id="refresh-page"><i class="ri-refresh-line mr-2"></i>Refresh</button>
                        </div>
                    </div>
                </div>
            </div>
            <!-- End Breadcrumbbar -->
            <!-- Start Contentbar -->
            <div class="contentbar">
                @RenderBody()
            </div>
            <!-- End Contentbar -->
            <!-- Start Footerbar -->
            <div class="footerbar">
                <footer class="footer">
                    <p class="mb-0">© 2020 Olian - All Rights Reserved.</p>
                </footer>
            </div>
            <!-- End Footerbar -->
        </div>
        <!-- End Rightbar -->
    </div>
    <!-- End Containerbar -->
    <!-- Start js -->
    <script src="~/Areas/UserBackend/Content/assets/js/jquery.min.js"></script>
    @RenderSection("scripts", required: false)
    <script src="~/Areas/UserBackend/Content/assets/js/popper.min.js"></script>
    <script src="~/Areas/UserBackend/Content/assets/js/bootstrap.min.js"></script>
    <script src="~/Areas/UserBackend/Content/assets/js/modernizr.min.js"></script>
    <script src="~/Areas/UserBackend/Content/assets/js/detect.js"></script>
    <script src="~/Areas/UserBackend/Content/assets/js/jquery.slimscroll.js"></script>
    <script src="~/Areas/UserBackend/Content/assets/js/vertical-menu.js"></script>
    <!-- Switchery js -->
    <script src="~/Areas/UserBackend/Content/assets/plugins/switchery/switchery.min.js"></script>
    <!-- Apex js -->
    <script src="~/Areas/UserBackend/Content/assets/plugins/apexcharts/apexcharts.min.js"></script>
    <script src="~/Areas/UserBackend/Content/assets/plugins/apexcharts/irregular-data-series.js"></script>
    <!-- Slick js -->
    <script src="~/Areas/UserBackend/Content/assets/plugins/slick/slick.min.js"></script>
    <!-- Custom Dashboard js -->
    <script src="~/Areas/UserBackend/Content/assets/js/custom/custom-dashboard.js"></script>
    <!-- Core js -->
    <script src="~/Areas/UserBackend/Content/assets/js/core.js"></script>
    <!-- End js -->
    <script>
        $("#refresh-page").click(function () {
            location.reload()
        })
    </script>
</body>

